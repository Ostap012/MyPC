<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:MyPC.ViewModels"
                    xmlns:view="clr-namespace:MyPC.Views">
    
    
    <DataTemplate DataType="{x:Type vm:SummaryVM}">
        <view:SummaryView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SensorsVM}">
        <view:SensorsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AboutVM}">
        <view:AboutView/>
    </DataTemplate>

</ResourceDictionary>